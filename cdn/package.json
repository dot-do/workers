{
  "name": "@dot-do/cdn",
  "version": "1.0.0",
  "description": "CDN Service - Content delivery network with event tracking, provenance, and analytics",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:migrations": "wrangler d1 migrations apply cdn-cache-db --local",
    "d1:migrations:remote": "wrangler d1 migrations apply cdn-cache-db --remote",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "hono": "^4.9.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  }
}
