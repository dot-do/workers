  "main": "index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": "./index.ts",
    "./env": "./env.ts",
    "./env/node": "./env-node.ts",
    "./dotenv": "./dotenv.ts",
    "./fn": "./fn-proxy.ts",
    "./sql": "./sql-proxy.ts",
    "./sql-proxy": "./sql-proxy.ts",
    "./tagged": "./tagged.ts"
  },
  "author": ".do",
  "license": "MIT",
  "homepage": "https://rpc.do",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/workers"
  },
