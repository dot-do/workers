    "test:watch": "vitest"
  },
  "dependencies": {
    "typescript": "^5.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "tsup": "^8.5.1",
    "vitest": "^3.2.4"
  }
