{
  "name": "@dotdo/rate-limiting",
  "version": "0.0.1",
  "description": "Rate limiting middleware for Cloudflare Workers with fail-closed option",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "rate-limiting",
    "middleware"
  ],
  "license": "MIT"
}
