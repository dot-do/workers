{
  "name": "@dotdo/observability",
  "version": "0.0.1",
  "description": "Observability utilities for Cloudflare Workers - metrics, tracing, and lifecycle hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "vitest": "^3.2.4",
    "typescript": "^5.6.0",
    "tsup": "^8.5.1"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "observability",
    "metrics",
    "tracing",
    "opentelemetry",
    "prometheus"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT"
}
