{
  "name": "workers.do",
  "version": "0.0.1",
  "description": "CLI tool for workers.do - Deploy snippets, workers, and manage .do services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "workers.do": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "capnweb": "^0.2.0",
    "cli.do": "workspace:*",
    "commander": "^12.0.0",
    "mcp.do": "workspace:*",
    "org.ai": "workspace:*",
    "rpc.do": "workspace:*"
  },
  "devDependencies": {
    "@dotdo/types": "workspace:*",
    "@types/node": "^20.0.0",
    "dotdo": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "snippets",
    "deployment",
    "cli",
    "workers.do"
  ],
  "files": [
    "dist"
  ]
}
