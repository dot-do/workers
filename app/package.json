{
	"name": "@do/app",
	"version": "1.0.0",
	"description": "Admin CMS worker - proxies to Payload CMS",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev",
		"build": "tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js",
		"deploy": "wrangler deploy",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit"
