  "devDependencies": {
    "@dotdo/types": "workspace:*",
    "capnweb": "^0.2.0",
    "dotdo": "workspace:*",
    "dotenv": "^16.0.0",
    "org.ai": "workspace:*",
    "typescript": "^5.0.0",
