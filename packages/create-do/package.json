  },
  "keywords": [
    "cloudflare",
    "durable-objects",
    "workers",
    "edge",
    "create",
    "cli",
    "scaffold"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "tsup": "^8.5.1",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "prompts": "^2.4.2",
    "picocolors": "^1.1.1",
    "fs-extra": "^11.2.0"
  }
}
