{
  "name": "create-do",
  "version": "0.0.1",
  "description": "Create a new .do service - Durable Object powered edge services",
  "type": "module",
  "bin": {
    "create-do": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
