 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "pipeline",
	"main": "worker.ts",
	"compatibility_date": "2025-07-08",
	"observability": { "enabled": true },
	"pipelines": [
    {
      "pipeline": "events-realtime",
      "binding": "pipeline"
    }
  ]
}
