{
  "name": "@dotdo/middleware",
  "version": "0.0.1",
  "description": "Middleware for Cloudflare Workers - Auth, Rate Limiting, etc.",
  "author": "Nathan Clevenger",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^20.19.27",
    "tsup": "^8.5.1",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "middleware",
    "auth",
    "hono"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
