	"dependencies": {
		"hono": "^4.7.11"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250110.0",
		"@types/node": "^22.0.0",
		"esbuild": "^0.24.2",
		"typescript": "^5.7.3",
		"vitest": "^3.2.3",
		"wrangler": "^3.103.0"
	},
	"engines": {
		"node": ">=20"
	}
}
