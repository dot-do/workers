{
  "name": "@dotdo/make",
  "version": "0.0.1",
  "description": "Make.com (Integromat) on Cloudflare - Visual scenario builder with advanced data routing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./do": {
      "types": "./dist/durable-object/index.d.ts",
      "import": "./dist/durable-object/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./modules": {
      "types": "./dist/modules/index.d.ts",
      "import": "./dist/modules/index.js"
    },
    "./hono": {
      "types": "./dist/hono/index.d.ts",
      "import": "./dist/hono/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "lint": "eslint src test --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "make",
    "integromat",
    "automation",
    "workflow",
    "scenario",
    "cloudflare",
    "durable-objects",
    "edge",
    "mcp",
    "ai",
    "visual-builder",
    "data-routing"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/make"
  },
  "homepage": "https://github.com/dot-do/make#readme",
  "bugs": {
    "url": "https://github.com/dot-do/make/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "peerDependencies": {
    "@dotdo/fsx": "^0.0.1",
    "@dotdo/gitx": "^0.0.1",
    "llm.do": "^0.0.1"
  },
  "peerDependenciesMeta": {
    "@dotdo/fsx": {
      "optional": true
    },
    "@dotdo/gitx": {
      "optional": true
    },
    "llm.do": {
      "optional": true
    }
  }
}
