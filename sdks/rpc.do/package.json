  "dependencies": {
    "@dotdo/types": "workspace:*"
  },
  "peerDependencies": {
    "capnweb": "^0.2.0",
    "dotenv": "^16.0.0",
    "org.ai": "workspace:*"
  },
  "peerDependenciesMeta": {
    "dotenv": {
      "optional": true
    }
