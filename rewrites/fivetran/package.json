{
  "name": "@dotdo/fivetran",
  "version": "0.0.1",
  "description": "Fivetran on Cloudflare - Automated data movement with zero maintenance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./connectors/*": {
      "types": "./dist/connectors/*.d.ts",
      "import": "./dist/connectors/*.js"
    },
    "./destinations/*": {
      "types": "./dist/destinations/*.d.ts",
      "import": "./dist/destinations/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fivetran",
    "etl",
    "data-pipeline",
    "cloudflare",
    "durable-objects",
    "data-integration",
    "connectors"
  ],
  "author": "workers.do",
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
