{
  "name": "@dot-do/workers",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.2",
  "scripts": {
    "create-service": "tsx scripts/create-service.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml}\"",
    "typecheck": "pnpm -r typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --run unit",
    "test:integration": "vitest run --config tests/integration/vitest.config.ts",
    "test:integration:watch": "vitest watch --config tests/integration/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "build": "pnpm -r build",
    "dev": "pnpm -r dev"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.19",
    "@cloudflare/workers-types": "^4.20250106.0",
    "@playwright/test": "^1.48.0",
    "@vitest/coverage-v8": "^2.1.8",
    "prettier": "^3.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 160
  }
}
